{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] [{Module}] {Message} {Properties}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "EmailsConfiguration": {
    "FromEmail": "no-reply@mymeetings.com",
    "FromName": "MyMeetings"
  },
  "JwtSettings": {
    "Authority": "https://localhost:5001",
    "ClientId": ""
  },
  "SiteSettings": {
    "SiteUrl": "https://localhost:7273"
  },
  "SmtpSettings": {
    "Host": "localhost",
    "Port": 1025,
    "Username": "TestSmtp",
    "Password": "TestPassword"
  }
}
